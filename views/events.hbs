{{#each events}}
  <h3>{{event_name}}</h3>
  <p>Location: {{location_name}}</p>
  <p>Date: {{date}}</p>
  <p>Price: ${{price}}</p>
  <p>Type: {{type}}</p>
  <p>Description: {{description}}</p>
{{/each}}
<div>
  <button class='btn' name='add' value='Submit'><a href='/events/add'>Add Event</a></button>
</div>